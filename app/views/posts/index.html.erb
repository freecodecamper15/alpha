<table class='table'>
  <tr>
    <th>Title</th>
    <th>Time Created</th>
    <th>Post Age</th>
  </tr>

  <% @posts.each do |post| %>
    <tr>
      <td><%= link_to post.title, post %></td>
      <td><%= post.created_at %></td>
      <td><%= time_ago_in_words(post.created_at) %></td>
    </tr>
  <% end %>
</table>
<%= paginate @posts %>